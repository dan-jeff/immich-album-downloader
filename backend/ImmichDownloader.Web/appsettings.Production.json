{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Error"
    }
  },
  "Jwt": {
    "Key": "${JWT_SECRET_KEY}",
    "ExpireMinutes": 30
  },
  "ConnectionStrings": {
    "DefaultConnection": "${CONNECTION_STRING}"
  },
  "Immich": {
    "Url": "${IMMICH_URL}",
    "ApiKey": "${IMMICH_API_KEY}"
  },
  "AllowedOrigins": ["${ALLOWED_ORIGINS}"],
  "DataPath": "${DATA_PATH:/app/data}"
}